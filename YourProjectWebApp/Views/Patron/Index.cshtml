@model IEnumerable<YourProjectWebApp.WebServiceYourProject.Patron>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<p class="alert-success">@TempData["Success"]</p> 
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PatronName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsGroup)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PatronName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsGroup)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
